<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.secmem_dy.sleep_assistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <receiver android:name=".AlarmReceiver" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AlarmSettingActivity">
            <intent-filter>
                    <action android:name="android.intent.action.AlarmSettingActivity">
                    <category android:name="android.intent.category.DEFAULT">
                    </category></action>
            </intent-filter>
        </activity>

        <activity android:name=".ShowDataActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.ShowDataActivity">
                    <category android:name="android.intent.category.DEFAULT">
                    </category></action>
            </intent-filter>
        </activity>

        <service android:name=".SleepDataService" android:enabled="true">
        </service>
        <service android:name=".WhiteNoiseService" android:enabled="true">
        </service><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>
    <uses-permission
        android:name="android.permission.INTERNET"></uses-permission>
</manifest>
